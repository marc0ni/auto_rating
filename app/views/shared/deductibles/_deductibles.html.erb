<%= form_for :deductibles  do |f|%>
	
<form action="#" class="form-horizontal">
	<% if @submission.risk == "Private Passenger"%>
		<%= render :partial => "shared/deductibles/pp_ded", :collection => @deductibles %>
		
	<% else %>
		<%= render :partial => "shared/deductibles/ttt_ded", :collection => @deductibles %>
		
	<% end %>
	<!--/row-->
	<div class="form-actions">
		<button type="submit" class="btn blue", deductibles_path(submission_id: submission)><i class="icon-ok"></i> Save</button>
		<button type="button" class="btn">Cancel</button>
	</div>	
</form>
  
<% end %>