<%= form_for :private_passenger, remote: true do |f|%>
<div class="container-fluid">
	<!-- BEGIN FORM-->
	<form action="#" class="form-horizontal">
		<div class="row-fluid">
			<!--/span -->
			<div class="span4 ">
				<div class="control-group">
					<label class="control-label">Primary Code</label>
					<div class="controls">
						<input type="text" class="m-wrap span12" <%= f.text_field :primary_code %>
					</div>
				</div>
			</div>
			<!--/span-->
			<div class="span4 ">
				<div class="control-group">
					<label class="control-label">Age Group</label>
					<div class="controls">
						<select class="m-wrap span12"<%= f.select :age_group, [ '1', '2', '3', '4', '5', '6'], :prompt => 'Select One' %>
						</select>
					</div>
				</div>
			</div>
			<!--/span-->
			<div class="span4 ">
				<div class="control-group">
					<label class="control-label" >Cost New</label>
					<div class="controls">
						<select class="m-wrap span12"<%= f.select :cost_new, [ '0-4500', '4501-6000', '6001-8000', '8001-10,000', '10,001-15,000','15,001-20,000', '20,001-25,000', '25,001-40,000', '41,001-65,000', 'over 65,000'], :prompt => 'Select One' %>
						</select>
					</div>
				</div>
			</div>
			
			<!--row 2-->
			<div class="row-fluid">
				<div class="span4 ">
					<div class="control-group">
						<label class="control-label">Make</label>
						<div class="controls">
							<input type="text" class="m-wrap span12" <%= f.text_field :make %>
						</div>
					</div>
				</div>
				<!--/span-->
				<div class="span4 ">
					<div class="control-group">
						<label class="control-label">Model</label>
						<div class="controls">
							<input type="text" class="m-wrap span12" <%= f.text_field :model %>
						</div>
					</div>
				</div>
				<!--/span-->
				<div class="span4 ">
					<div class="control-group">
						<label class="control-label">Year</label>
						<div class="controls">
							<input type="text" class="m-wrap span12" <%= f.text_field :model_year %>
						</div>
					</div>
				</div>
			</div>
		</div>
	</form>
</div>
  
<% end %>

